#!ipxe

isset ${dhcp-server} && set ipparam ip=dhcp || set ipparam ip=${ip}::${gateway}:${netmask}:::none nameserver=${dns}
set ipparam BOOTIF=${netX/mac} ${ipparam}

goto ${menu} ||

:live_menu
set os Fedora Live
menu ${os} - Current Arch [ ${arch} ]
iseq ${arch} x86_64 && set arch_a amd64 || set arch_a ${arch}
item --gap ${os} Versions
item 33 ${space} ${os} 33
item 32 ${space} ${os} 32
item 31 ${space} ${os} 31
choose live_version || goto live_exit
menu ${os} ${live_version}
item --gap ${os} Flavors
goto ${live_version}

:33
item fedora-33-Cinnamon ${space} Fedora 33 Cinnamon(local)
item fedora-33-KDE ${space} Fedora 33 Kde(local)
item fedora-33-LXDE ${space} Fedora 33 Lxde(local)
item fedora-33-LXQt ${space} Fedora 33 Lxqt(local)
item fedora-33-MATE_Compiz ${space} Fedora 33 Mate_compiz (local)
item fedora-33-SoaS ${space} Fedora 33 Soas (local)
item fedora-33-Xfce ${space} Fedora 33 Xfce (local)
item fedora-33-gnome ${space} Fedora 33 Gnome (local)
goto flavor_select
:32
item fedora-32-Cinnamon ${space} Fedora 32 Cinnamon(local)
item fedora-32-KDE ${space} Fedora 32 Kde(local)
item fedora-32-LXDE ${space} Fedora 32 Lxde(local)
item fedora-32-LXQt ${space} Fedora 32 Lxqt(local)
item fedora-32-MATE_Compiz ${space} Fedora 32 Mate_compiz(local)
item fedora-32-SoaS ${space} Fedora 32 Soas(local)
item fedora-32-Xfce ${space} Fedora 32 Xfce(local)
item fedora-32-gnome ${space} Fedora 32 Gnome(local)
