#!ipxe

goto ${menu} ||

:live_menu
set os Fatdog64
menu ${os} - Current Arch [ ${arch} ]
iseq ${arch} x86_64 && set arch_a amd64 || set arch_a ${arch}
item --gap ${os} Versions
item current ${space} ${os} current
choose live_version || goto live_exit
goto ${live_version}


:current
set url ${live_endpoint}/asset-mirror/releases/download/811-5095c9db/
goto boot


:boot
imgfree
kernel ${url}vmlinuz rootfstype=ramfs initrd=initrd ${cmdline}
initrd ${url}initrd
boot

:live_exit
clear menu
exit 0
