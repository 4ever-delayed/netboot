#!ipxe

goto ${menu} ||

:live_menu
set os Pop os Live
menu ${os} - Current Arch [ ${arch} ]
iseq ${arch} x86_64 && set arch_a amd64 || set arch_a ${arch}
item --gap ${os} Versions
item focal ${space} ${os} 20.04
item bionic ${space} ${os} 18.04
item eoan ${space} ${os} 19.10
choose live_version || goto live_exit
menu ${os} ${live_version}
item --gap ${os} Flavors
goto ${live_version}

:focal
item pop-20.04-default-squash ${space} Pop 20.04 Intel-Amd
item pop-20.04-nvidia-squash ${space} Pop 20.04 Nvidia(local)
goto flavor_select

:bionic
item pop-18.04-default-squash ${space} Pop 18.04 Intel-Amd
item pop-18.04-nvidia-squash ${space} Pop 18.04 Nvidia
goto flavor_select

:eoan
item pop-19.10-default-squash ${space} Pop 19.10 Intel-Amd
item pop-19.10-nvidia-squash ${space} Pop 19.10 Nvidia
goto flavor_select

:flavor_select
choose flavor || goto live_menu
echo ${cls}
goto ${flavor} ||

:pop-18.04-default-squash
set squash_url ${live_endpoint}/ubuntu-squash/releases/download/58-600b3df6/filesystem.squashfs
set kernel_url ${live_endpoint}/ubuntu-squash/releases/download/58-600b3df6/
goto 18.04-boot
:pop-18.04-nvidia-squash
set squash_url ${live_endpoint}/ubuntu-squash/releases/download/58-316fd7f7/filesystem.squashfs
set kernel_url ${live_endpoint}/ubuntu-squash/releases/download/58-316fd7f7/
goto 18.04-boot
:pop-19.10-default-squash
set squash_url ${live_endpoint}/ubuntu-squash/releases/download/10-bdc3fe0a/filesystem.squashfs
set kernel_url ${live_endpoint}/ubuntu-squash/releases/download/10-bdc3fe0a/
goto 19.10-boot
:pop-19.10-nvidia-squash
set squash_url ${live_endpoint}/ubuntu-squash/releases/download/10-3882d464/filesystem.squashfs
set kernel_url ${live_endpoint}/ubuntu-squash/releases/download/10-3882d464/
goto 19.10-boot
:pop-20.04-default-squash
set squash_url ${live_endpoint}/ubuntu-squash/releases/download/5-42ebbb8e/filesystem.squashfs
set kernel_url ${live_endpoint}/ubuntu-squash/releases/download/5-42ebbb8e/
goto 20.04-boot
:pop-20.04-nvidia-squash
set squash_url http://192.168.1.120:8080/ubuntu-squash/releases/download/5-ebd6853e/filesystem.squashfs
set kernel_url http://192.168.1.120:8080/ubuntu-squash/releases/download/5-ebd6853e/
goto 20.04-boot

:18.04-boot
imgfree
kernel ${kernel_url}vmlinuz ip=dhcp boot=casper netboot=http fetch=${squash_url} initrd=initrd ${cmdline}
initrd ${kernel_url}initrd
boot

:19.10-boot
imgfree
kernel ${kernel_url}vmlinuz ip=dhcp boot=casper netboot=url url=${squash_url} initrd=initrd ${cmdline}
initrd ${kernel_url}initrd
boot

:20.04-boot
imgfree
kernel ${kernel_url}vmlinuz ip=dhcp boot=casper netboot=url url=${squash_url} initrd=initrd ${cmdline}
initrd ${kernel_url}initrd
boot

:live_exit
clear menu
exit 0

